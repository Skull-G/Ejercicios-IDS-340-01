using System;

class ProgramaFor
{
    static void Main(string[] args)
    {
        Console.Write("Ingresa un n√∫mero para mostrar su tabla de multiplicar: ");
        int numero = int.Parse(Console.ReadLine());

        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine($"{numero} x {i} = {numero * i}");
        }
    }
}
